{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "es2017"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": false,
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
